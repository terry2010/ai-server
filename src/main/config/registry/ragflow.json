{
  "name": "ragflow",
  "type": "feature",
  "dependsOn": ["mysql", "redis", "minio", "elasticsearch"],
  "ports": [
    { "container": 9380, "host": "${RAGFLOW_PORT:18600}", "bind": "${BIND_ADDRESS:127.0.0.1}" }
  ],
  "variables": {
    "RAGFLOW_PORT": "${RAGFLOW_PORT:18600}",
    "BIND_ADDRESS": "${BIND_ADDRESS:127.0.0.1}"
  },
  "healthCheck": { "type": "http", "target": "http://localhost:${RAGFLOW_PORT:18600}/", "interval": 2000, "timeout": 20000, "retries": 30 },
  "compose": { "templateRef": "docker-compose.feature.yml" }
}
